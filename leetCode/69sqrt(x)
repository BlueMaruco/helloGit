public class Solution {
   public int mySqrt(int x){
    	int lo = 0;
    	int hi = Integer.MAX_VALUE;
    	
    	while(lo<=hi){
            int medium = lo + (hi-lo)/2;
    		long product = (long)medium*medium;
    		if(product == x) return medium;
    		else if(product<x){
    			if((long)(medium+1)*(medium+1)>x)
    				return medium;
    			else{
    	            lo = medium+1;
                }
    	
            }else{
            	hi = medium-1;
            }
    
        }
        return x;
    }

}
