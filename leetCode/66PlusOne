public class Solution {
    public int[] plusOne(int[] digits){
	int plus = 1;
	for(int i = digits.length-1;i>=0;i--){
        if(digits[i]+plus>9){
	        if(i==0){
		        int[] newdigits = new int[digits.length+1];
                	newdigits[0]=1;
                	return newdigits;
            }else{
		digits[i]=0;
            }
        }else{
        	digits[i]=digits[i]+plus;
            return digits;
        }
    }
return digits;
}

}
